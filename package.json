{
  "name": "@gns-science/toshi-nest",
  "version": "1.1.2",
  "description": "The toshi-nest is for fledgling work e.g. reusable Node components to share across TUI and other projects",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "repository": "https://github.com/GNS-Science/toshi-nest.git",
  "author": "GNS Science",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "start-playground": "cd playground && yarn run start",
    "start-playground-ts": "cd playground-ts && yarn run start", 
    "i-all": "yarn && cd playground && yarn && cd .. && cd playground-ts && yarn",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "dev-ts": "npm-run-all --parallel build-watch start-playground-ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint ./src/**/*.{js,ts,tsx} --quiet --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,ts,tsx}": [
      "eslint --quiet --fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@react-three/fiber": "6.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.42",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "yarn-run-all": "^3.1.1",
    "react-konva": "^17.0.0-0",
    "three": "0.126",
    "zdog": "1.0"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.2",
    "@visx/clip-path": "^2.1.0",
    "@visx/group": "^2.1.0",
    "@visx/legend": "^2.2.2",
    "@visx/responsive": "^2.8.0",
    "@visx/scale": "^2.2.2",
    "@visx/xychart": "^2.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-spring": "^9.4.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.2",
    "@visx/clip-path": "^2.1.0",
    "@visx/group": "^2.1.0",
    "@visx/legend": "^2.2.2",
    "@visx/responsive": "^2.8.0",
    "@visx/scale": "^2.2.2",
    "@visx/xychart": "^2.9.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-spring": "^9.4.4"
  }
}
